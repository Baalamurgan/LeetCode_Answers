class Solution {
    public int[] twoSum(int[] nums, int target) {
        Map<Integer,Integer> pos = new HashMap<>();
        for(int i=0;i<nums.length;pos.put(nums[i],i),++i){
            if(pos.containsKey(target-nums[i])){
                return new int[]{pos.get(target-nums[i]),i};
            }
        }
        return new int[]{};
    }
}
