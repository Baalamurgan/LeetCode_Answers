class Solution {
    public int removeDuplicates(int[] nums) {
        int count=1;
        if(nums.length>0){
            int k=nums[0];
            for(int i=1;i<nums.length;++i){
                if(nums[i] != k){
                    k=nums[i];
                    nums[count]=k;
                    count++;
                }
            }
        }
        else{
            return 0;
        }
        return count;
    }
}
